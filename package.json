{
  "name": "tthk-api",
  "module": "main.ts",
  "type": "module",
  "scripts": {
    "start": "bun --env-file=.env ./src/main.ts",
    "dev": "bun run --env-file .env.development --watch ./src/main.ts",
    "build-docs": "bun scripts/build-docs.js",
    "generate:dev": "bun --env-file .env.development run scripts/generate-types.ts",
    "generate:prod": "bun --env-file .env.development run scripts/generate-types.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "ttypescript": "^1.5.15"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@redocly/cli": "^1.14.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.10",
    "@types/shelljs": "^0.8.15",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "node-html-parser": "^6.1.13",
    "shelljs": "^0.8.5",
    "swagger-autogen": "^2.23.7"
  }
}
